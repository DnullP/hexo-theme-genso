.navigator {
    width: 100%;
    height: 6.888%;
    display: flex;
    justify-content: center;
    position: fixed;
    top: 0;
    z-index: 100;
    background: var(--rainbow, linear-gradient(90deg, #FFD3D3 0%, rgba(255, 245, 218, 0.83) 25.83%, #F4FFE3 47.19%, #F3F3FF 64.9%, #EDEEFF 80.29%, #FADDFF 100%));
    flex-shrink: 0;
}

.page-container {
    width: 100%;
    height: auto;
    display: flex;
    flex-direction: row;
    justify-content: center;
    gap: 50px;
    margin-top: 0;
    margin-right: 0;
    margin-bottom: 0;
    margin-left: 0;
}

//This is used to fill the absense of the background image
.background-occupation {
    width: 100%;
    height: 100vh;
    display: flex;
    flex-direction: column;
    justify-content: end;
}

.background {
    width: 100%;
    height: 100%;
    position: fixed;
    top: 0;
    left: 0;
    z-index: -100;
    overflow: hidden;
    background: #ae4848;
}

.background img {
    object-fit: cover;
    width: 100%;
    height: 100%;
}

.background-coverage {
    position: fixed;
    background: rgba(255, 255, 255, 0.82);
    width: 100%;
    bottom: 0;
    left: 0;
    z-index: -99;
    height: 38.2%;
    overflow: hidden;
}

//this is the style of the typed text.
.background-coverage span{
    width: auto
    height: auto;
    flex-shrink: 0;
    color: #4C495F;
    font-family: Inter;
    font-size: 64px;
    font-style: normal;
    font-weight: 700;
    line-height: normal;
}


